{
  "name": "@saghen/string-tracker",
  "version": "0.0.1-rc4",
  "description": "A library for operating on strings while maintaining changes and index maps transparently",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -r dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json && ./fix-up.sh",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saghen/string-tracker.git"
  },
  "author": "Liam Dyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Saghen/string-tracker/issues"
  },
  "homepage": "https://github.com/Saghen/string-tracker#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "ts-jest": "^27.0.4",
    "typescript": "^4.4.1-rc"
  }
}
